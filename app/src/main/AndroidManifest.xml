<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jefrienalvizures.tonechord">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name=".servicios.MensajeService" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".NuevaActivity" />
        <activity android:name=".ChordActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".DonateActivity" />
        <activity android:name=".EliminarCuenta" />
        <activity android:name=".DetalleChordActivity" />
        <activity android:name=".MensajesActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity android:name=".EnviarMensajeActivity"></activity>

    </application>

</manifest>